{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "strict": true,
    "alwaysStrict": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "emitDeclarationOnly": true,
    "verbatimModuleSyntax": false
  }
}
